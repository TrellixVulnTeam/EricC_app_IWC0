{
  "expo": {
    "name": "ECapp",
    "slug": "ECapp",
    "owner": "yeoldeericc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#110f14"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/*",
      "tmp/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#110f14"
      },
      "package": "com.yeoldeericc.ECapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
